<a href={this.model.href} class="brand-profile"
  style="background-image: url({this.model.background || ''})">
  <img src={this.model.avatar} class="brand-profile-avatar">
  <div class="brand-profile-name">{this.model.name}</div>
  <div class="brand-profile-location">
    <i class="fa fa-map-marker"></i>
    <span>{this.model.location}</span>
  </div>
</a>

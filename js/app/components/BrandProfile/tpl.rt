<a href="##" class="brand-profile"
  style="background-image: url({this.props.background})">
  <img src="{this.props.avatar}" class="brand-profile-avatar">
  <div class="brand-profile-name">{this.props.name}</div>
  <div class="brand-profile-location">
    <i class="fa fa-map-marker"></i>
    <span>{this.props.location}</span>
  </div>
</a>
